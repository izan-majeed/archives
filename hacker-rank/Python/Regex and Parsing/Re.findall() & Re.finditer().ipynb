   "source": [
    "import re\n",
    "text = input()\n",
    "\n",
    "#?<= lookbehind assertion\n",
    "#?= lookahead assertio\n",
    "pattern = r'(?<=[qwrtypsdfghjklzxcvbnm])([aeiou]{2,})(?=[qwrtypsdfghjklzxcvbnm])'\n",
    "\n",
    "result = re.findall(pattern, text, re.I)\n",
    "\n",
    "if result:\n",
    "    print(*result, sep='\\n')\n",
    "else:\n",
    "    print(-1)"
   ]
