   "source": [
    "for _ in range(int(input())):\n",
    "    n = int(input())\n",
    "    result = -1\n",
    "    for a in range(3, (n//3)+1):\n",
    "        b = (n**2 - 2*a*n) // (2*n - 2*a)\n",
    "        c = n - b - a\n",
    "        if (a**2 + b **2 == c**2) and (a*b*c > result):\n",
    "            result = a*b*c\n",
    "    print(result)"
   ]
