   "source": [
    "from itertools import permutations\n",
    "s, v = input().split()\n",
    "l = list(permutations(s,int(v)))\n",
    "l = list(map(list, l))\n",
    "l = sorted(list(map(''.join, l)))\n",
    "for i in l:\n",
    "    print(i)"
