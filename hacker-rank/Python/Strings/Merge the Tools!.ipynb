    "def merge_the_tools(string, k):\n",
    "    n = len(string)\n",
    "    for i in range(0,n-k+1,k):\n",
    "        temp = string[i:i+k]\n",
    "        temp = sorted(set(temp), key=temp.index)\n",
    "        print(''.join(temp))"
