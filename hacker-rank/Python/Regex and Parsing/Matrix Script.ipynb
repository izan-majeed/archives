   "source": [
    "import re\n",
    "\n",
    "N,M = map(int, input().split())\n",
    "matrix = [input() for _ in range(N)]\n",
    "text = ''\n",
    "\n",
    "for i in zip(*matrix):\n",
    "    text += \"\".join(i)\n",
    "        \n",
    "print(re.sub(r'(?<=\\w)([^\\w\\d]+)(?=\\w)', ' ', text))"
   ]
