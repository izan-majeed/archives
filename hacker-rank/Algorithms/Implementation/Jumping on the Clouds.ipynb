    "def jumpingOnClouds(c):\n",
    "    n = len(c)\n",
    "    i = 0\n",
    "    count = -1\n",
    "    \n",
    "    while (i < n):\n",
    "        if i<n-2 and c[i+2]==0:\n",
    "            i += 1\n",
    "        count += 1\n",
    "        i += 1\n",
    "        \n",
    "    return count"
   ]
  }
