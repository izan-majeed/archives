   "source": [
    "from statistics import median\n",
    "\n",
    "n = int(input())\n",
    "X = list(map(float, input().split()))\n",
    "F = list(map(int, input().split()))\n",
    "A = sorted([X[i] for i in range(n) for _ in range(F[i])])\n",
    "\n",
    "m = len(A)\n",
    "q1 = median(A[:m//2])\n",
    "q3 = median(A[(m+1)//2:])\n",
    "\n",
    "print(q3-q1)"
   ]
