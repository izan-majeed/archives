    "def dayOfProgrammer(year):\n",
    "    if year%4==0 and (year<1918 or year%400==0 or year%100!=0):\n",
    "        return f\"12.09.{year}\"\n",
    "\n",
    "    elif year == 1918:\n",
    "        return \"26.09.1918\"\n",
    "\n",
    "    return f\"13.09.{year}\"\n",
    "\n",
    "\n",
    "year = int(input().strip())\n",
    "result = dayOfProgrammer(year)\n",
    "print(result)"
   ]
