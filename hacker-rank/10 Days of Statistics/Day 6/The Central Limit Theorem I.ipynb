    "from math import erf\n",
    "x, n = 9800, 49\n",
    "mean, std = 205, 15\n",
    "mean_sum = n * mean \n",
    "std_sum = (n**0.5) * (std)\n",
    "\n",
    "cdf = lambda x: 0.5 * (1 + erf((x - mean_sum) / (std_sum * (2 ** 0.5))))\n",
    "\n",
    "print(round(cdf(x), 4))"
   ]
