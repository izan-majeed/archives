    "import math\n",
    "\n",
    "def p(k, l):\n",
    "    return ( (l**k)*(math.e**(-l)) ) / math.factorial (k)\n",
    "\n",
    "l, k = [float(input()) for i in range(2)]\n",
    "print(round(p(k, l) , 3))"
   ]
