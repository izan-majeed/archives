   "source": [
    "from itertools import combinations\n",
    "\n",
    "n = int(input())\n",
    "l = input().split()\n",
    "k = int(input())\n",
    "count = 0\n",
    "\n",
    "for t in combinations(l,k):\n",
    "    if 'a' in t:\n",
    "        count+=1\n",
    "        \n",
    "result = count/len(list(combinations(l, k)))\n",
    "print(f\"{result:.4f}\")"
   ]
