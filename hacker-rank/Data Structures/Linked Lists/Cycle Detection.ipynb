    "def has_cycle(head):\n",
    "    if not head:\n",
    "        return 0\n",
    "    \n",
    "    slow, fast = head, head\n",
    "    while (fast and fast.next):\n",
    "        slow = slow.next\n",
    "        fast = fast.next.next\n",
    "        if slow == fast:\n",
    "            return 1\n",
    "\n",
    "    return 0"
   ]
