    "def reverse(head):\n",
    "    temp = head\n",
    "    prev = None\n",
    "    \n",
    "    while temp:\n",
    "        next_ = temp.next\n",
    "        temp.next = prev\n",
    "        temp.prev = next_\n",
    "        prev = temp;\n",
    "        temp = next_\n",
    "        \n",
    "    return prev"
   ]
