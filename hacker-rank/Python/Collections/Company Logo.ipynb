    "from collections import Counter, OrderedDict\n",
    "\n",
    "s = Counter(sorted(input()))\n",
    "d = OrderedDict(sorted(s.items(), key=lambda x: x[1], reverse=True))     #sort on keys\n",
    "for k,v in list(d.items())[:3]:\n",
    "    print(k,v)"
