   "source": [
    "from collections import namedtuple\n",
    "n = int(input())\n",
    "t = namedtuple('Student', input().strip())\n",
    "total = 0\n",
    "\n",
    "for _ in range(n):\n",
    "    a, b, c, d = input().split()\n",
    "    total += int(t(a,b,c,d).MARKS)\n",
    "\n",
    "print(total/n)"
   ]
