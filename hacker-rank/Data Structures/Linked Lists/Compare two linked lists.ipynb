    "def compare_lists(head1, head2):\n",
    "    temp1, temp2 = head1, head2\n",
    "    \n",
    "    while temp1 or temp2:\n",
    "        if not temp1 or not temp2: return 0\n",
    "        if temp1.data != temp2.data: return 0\n",
    "        temp1 = temp1.next\n",
    "        temp2 = temp2.next\n",
    "\n",
    "    return 1"
