    "import re\n",
    "\n",
    "text = input()\n",
    "pattern = input()\n",
    "\n",
    "for i in range(len(text)):\n",
    "    if re.match(pattern, text[i:]):\n",
    "        print((i, i+len(pattern)-1))\n",
    "        \n",
    "if not re.search(pattern, text):\n",
    "    print((-1, -1))  "
