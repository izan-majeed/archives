   "source": [
    "def count_substring(string, sub_string):\n",
    "    count = 0\n",
    "    n = len(sub_string)\n",
    "\n",
    "    for i in range(len(string)):\n",
    "        if (sub_string == string[i:i+n]):\n",
    "            count+=1\n",
    "    return count"
   ]
