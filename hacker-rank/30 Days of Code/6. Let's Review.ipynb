   "source": [
    "n = int(input())\n",
    "result = []\n",
    "for i in range(n):\n",
    "    s = input()\n",
    "    even = ''.join([s[i] for i in range(0,len(s),2)])\n",
    "    odd = ''.join([s[i] for i in range(1,len(s),2)])\n",
    "    result.append(f'{even} {odd}')\n",
    "\n",
    "for i in result:\n",
    "    print(''.join(i))"
   ]
