   "source": [
    "from math import erf\n",
    "mean, std = 20, 2\n",
    "cdf = lambda x: 0.5 * (1 + erf((x - mean) / (std * (2 ** 0.5))))\n",
    "\n",
    "print(round(cdf(19.5), 3))\n",
    "print(round((cdf(22) - cdf(20)), 3))"
   ]
