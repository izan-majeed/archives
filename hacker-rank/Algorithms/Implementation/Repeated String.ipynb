   "source": [
    "def repeatedString(s, n):\n",
    "    count = 0\n",
    "    for i in s:\n",
    "        if i == 'a':\n",
    "            count+=1\n",
    "       \n",
    "    x = n//len(s)\n",
    "    count *= x\n",
    "    \n",
    "    y = n%len(s)\n",
    "    for i in range(y):\n",
    "        if s[i] == 'a':\n",
    "            count+=1\n",
    "            \n",
    "    return count"
   ]
  }
