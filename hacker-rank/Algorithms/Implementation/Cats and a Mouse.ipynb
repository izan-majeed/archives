    "def catAndMouse(x, y, z):\n",
    "    x, y = abs(x-z), abs(y-z)\n",
    "    if x < y:\n",
    "        return \"Cat A\"\n",
    "    elif x > y:\n",
    "        return \"Cat B\"\n",
    "    return \"Mouse C\"\n",
    "\n",
    "\n",
    "for _ in range(int(input())):\n",
    "    x, y, z = map(int, input().split())\n",
    "    result = catAndMouse(x, y, z)\n",
    "    print(result)"
